; Ackermann function
;
; input:
;   $0 = i
;   $1 = n
;
; output:
;   $2 = A(i,n)
;
; temporary variables:
;   $3 = j
;   $4 = transferring
;   $5..$(6+i) = diff (array)
;   $(7+i)..$(8+2i) = next (array)

; set arguments
mov $0,3      ; i = 3
mov $1,3      ; n = 3

; initialize diff and next arrays
mov $3,$0     ; j = i
add $3,1      ; j = i+1
lpb $3,1       ; for j = i+1..1 do
  add $3,4    ;
  mov $$3,1   ;   diff[j] := 1
  add $3,$0   ;
  add $3,1    ;
  mov $$3,0   ;   next[j] := 0
  sub $3,$0   ;
  sub $3,6    ;
lpe           ; end for
add $5,$1     ; diff[0] := n+1

; main descent loop 
; lpb 5,


; lpe
