; Ackermann function
;
; Input:
;   $0 = i
;   $1 = n
;
; Output:
;   $2 = A(i,n)
;
; Temporary:
;   $3 = j
;   $4 = transferring
;   $5..$(6+i) = diff (array)
;   $(7+i)..$(8+2i) = next (array)


mov $0,5      ; i := 5
mov $1,7      ; n := 7

mov $3,$0     ; j := i
add $3,1      ; j := i+1
lpb 3,1       ; j := i+1..1
  add $3,5
  mov $$3,1   ; diff[j] := 1
  add $3,$0
  mov $$3,9   ; next[j] := 9
  sub $3,$0
  sub $3,6
lpe
add $8,$1     ; diff[0] := n+1



